<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alberth.appstopinfraction">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presenter.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".presenter.listInfraction.ListInfractionActivity" />
        <activity android:name=".presenter.typeInfractions.TypeInfractionsActivity" />
        <activity android:name=".presenter.queryPlaque.QueryPlaqueActivity" />
        <activity android:name=".presenter.register.RegisterActivity" />
        <activity android:name=".presenter.editInfraction.EditActivity"></activity>
    </application>

</manifest>